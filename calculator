#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter 2 numbers:");
    scanf("%d%d",&a,&b);
        printf("enter a option:");
        scanf("%d",&c);
    switch(c)
    {
        case 1:
        c=a+b;
        printf("%d",c);
        break;
        
        case 2:
        c=a*b;
        printf("%d",c);
        break;
        
        case 3:
        c=a%b;
        printf("%d",c);
        break;
        
        case 4:
        c=a/b;
        printf("%d",c);
        break;
        
        case 5:
        c=a-b;
        printf("%d",c);
        
    
        
    }
    
    return 0;
}
