#include<stdio.h>
#include<stdlib.h>
void print(int n,int* a)
{
    for(int i=0;i<n;i++)
        {
       printf("%d",*(a+i));
        }
}
int main()
{
    int n;
    scanf("%d",&n);
    int *a=(int*)calloc(n,sizeof(int));
    for(int i=0;i<n;i++)
    {
            scanf("%d",a+i);
    }
   print(n,a);
   return 0;
   
}
