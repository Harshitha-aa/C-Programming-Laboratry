#include <stdio.h>

int main()
{
    int n=5;
    int arr[]={1,2,2,4,4};
    int c=0;
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(arr[i]==arr[j])
            {
                c++;
            }
        }
        if(c==1)
        {
            printf("%d",arr[i]);
        }
        else
        {
            c=0;
        }
    }
    return 0;
}
