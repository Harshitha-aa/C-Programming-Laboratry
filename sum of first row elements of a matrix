#include<stdio.h>
int magic(int n,int m,int a[n][m])
{
    int sum;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            sum=sum+a[i][j];
            
        }
        break;
    }
    return sum;
}
int main()
{
    int n,m;
    scanf("%d%d",&n,&m);
    int a[n][m];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("%d",magic(n,m,a));
    return 0;
}
