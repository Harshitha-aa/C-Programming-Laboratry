#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a>b&&a>c)
    {
        printf("a is greatest");
    }
    else if(c>a&&c>b)
    
 {
        printf("c is greatest");
    }
    else if(b>a&&b>c)
    {
        printf("b is greatest");
    }
    if(a<b&&a<c)
    {
        printf("\na is smalest");
    }
    else if(c<a&&c<b)
    
 {
        printf("\nc is smallest");
    }
    else if(b<a&&b<c)
    {
        printf("\nb is smallest");
    }
    
    return 0;
}
