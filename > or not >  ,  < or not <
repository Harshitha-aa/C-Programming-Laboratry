#include<stdio.h>
int main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("\na is greater");
    }
    else
    {
        printf("\na is not greater");
    }
    if(b>a)
    {
        printf("\na is smaller");
    }
    else
    {
        printf("\na is not smaller");    }
    return 0;
}
